<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.futeboldospais.futeboldospais.controller.QuintaRodadaFragment">

    <!-- TODO: Update blank fragment layout -->
    <ListView
        android:id="@+id/quinta_rodada_tabela"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fastScrollEnabled="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:scrollbars="none"/>

    <ListView
        android:id="@+id/quinta_rodada_tabela_vazia"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fastScrollEnabled="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_below="@id/quinta_rodada_tabela"
        android:scrollbars="none"/>
</RelativeLayout>
